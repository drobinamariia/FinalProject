[run]
source = .
omit = 
    */migrations/*
    */venv/*
    */env/*
    */site-packages/*
    manage.py
    */settings/*
    */tests.py
    */test_*.py
    */__pycache__/*
    */node_modules/*
    .git/*
    */static/*
    */media/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    class .*\bProtocol.*:
    @(abc\.)?abstractmethod

show_missing = True
skip_covered = False
precision = 0

[html]
directory = coverage_html_report
title = ShareName API Coverage Report

[xml]
output = coverage.xml